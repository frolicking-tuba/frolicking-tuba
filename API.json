        {
          "title": "signup",
          "description": "create a user given an email and password.\nOn success (200) a cookie storing the user's session is returned along with a json object with error being null.\nOn error the status code 400 is returned with a json object indicating the error in a string.",
          "url": "/api/users/signup",
          "verb": "POST",
          "input": {
            "code": "javascript",
            "object": { "email": "String", "password": "String"}
          },
          "output": {
            "on success": {
              "code": "javascript",
              "text": "STATUS CODE 200",
              "object": {  "error": null }
            },
            "on error": {
              "code": "javascript",
              "text": "STATUS CODE 400",
              "object": {  "error": "String" }
            }
          },
          "auth": "NONE"
        },
        {
          "title": "signin",
          "description": "sign in an existing user using an email and password.\nOn success (200) a cookie storing the user's session is returned along with a json object with error being null.\nOn error the status code 400 is returned with a json object indicating the error in a string.",
          "url": "/api/users/signin",
          "verb": "POST",
          "input": {
            "code": "javascript",
            "object": { "email": "String", "password": "String" }
          },
          "output": {
            "on success": {
              "code": "javascript",
              "text": "STATUS CODE 200",
              "object": { "error": null }
            },
            "on error": {
              "code": "javascript",
              "text": "STATUS CODE 400",
              "object": { "error": "String" }
            }
          },
          "auth": "NONE"
        },
        {
          "title": "signout",
          "description": "destroyes the current user's session if it exists always returns 200",
          "verb": "GET",
          "input": { "code": "javascript", "text": "EMPTY" },
          "output": { "code": "javascript", "text": "EMPTY" },
          "auth": "NONE"
        }
      ]
    },
